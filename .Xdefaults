! ~/.Xdefaults
! $Date$

URxvt*color0:  #000000
URxvt*color1:  #cc0000
URxvt*color2:  #00cc00
URxvt*color3:  #cccc00
URxvt*color4:  #0000cc
URxvt*color5:  #cc00cc
URxvt*color6:  #00cccc
URxvt*color7:  #aaaaaa
URxvt*color8:  #333333
URxvt*color9:  #ff0000
URxvt*color10: #00ff00
URxvt*color11: #ffff00
URxvt*color12: #0000ff
URxvt*color13: #ff00ff
URxvt*color14: #00ffff
URxvt*color15: #ffffff

URxvt*colorBDMode: true
URxvt*colorBD: #ffffff
URxvt*background:black
URxvt*foreground:#A8A8A8

URxvt*inheritPixmap: true
URxvt*tintColor: white
URxvt*shading: 20

URxvt*scrollBar: true
URxvt*scrollBar_right: true
URxvt*scrollBar_floating: true
URxvt*scrollstyle: plain
URxvt*transparentScrollbar: true

URxvt*hideButtons: true
URxvt*transparentMenubar: true

URxvt*saveLines: 1000
URxvt*cursorColor: yellow

URxvt*visualBell: true

! default: `"'&()*,;<=>?@[]{|}
URxvt*cutchars: `"()'*<>[]{|}

URxvt*font: 6x13
URxvt*boldFont: 6x13

Xft.dpi: 75
